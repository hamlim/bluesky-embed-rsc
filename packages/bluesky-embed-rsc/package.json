    "name": "Matt Hamlin",
    "email": "matthewjameshamlin@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hamlim/bluesky-embed-rsc",
    "directory": "packages/bluesky-embed-rsc"
  },
