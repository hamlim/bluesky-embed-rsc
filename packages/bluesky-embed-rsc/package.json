  "type": "module",
  "devDependencies": {
    "hohoro": "0.3.0",
    "@types/bun": "1.1.11",
    "typescript": "5.4.5",
    "lucide-react": "0.461.0",
    "hls.js": "1.6.2",
    "@radix-ui/react-aspect-ratio": "1.1.6",
    "clsx": "2.1.1",
    "tailwind-merge": "3.3.0",
    "@atcute/bluesky": "3.0.0",
    "@atcute/bluesky-richtext-segmenter": "2.0.1",
    "@atcute/client": "4.0.0",
    "@atcute/lexicons": "1.0.0"
  },
  "peerDependencies": {
    "lucide-react": "^0.461.0",
    "react": "*",
    "hls.js": "^1.6.2",
    "@radix-ui/react-aspect-ratio": "^1.1.6",
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.3.0",
    "@atcute/bluesky": "^3.0.0",
